{
  "name": "aquarian-gnosis",
  "version": "1.0.0",
  "description": "A gnostic community platform for connecting seekers worldwide",
  "scripts": {
    "dev": "pm2 start ecosystem.config.js",
    "dev:frontend": "pm2 start ecosystem.config.js --only aquarian-gnosis-frontend",
    "dev:backend": "pm2 start ecosystem.config.js --only aquarian-gnosis-backend",
    "install:all": "npm install && cd client && npm install && cd ../server && pip install -r requirements.txt",
    "build": "cd client && npm run build",
    "test": "concurrently \"npm run test:backend\" \"npm run test:frontend\"",
    "test:backend": "cd server && pytest",
    "test:frontend": "cd client && npm test",
    "setup": "./setup.sh",
    "start": "./run.sh",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "gnosis",
    "community",
    "spiritual",
    "samael-aun-weor",
    "map",
    "networking"
  ],
  "author": "Aquarian Gnosis Development Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "puppeteer": "^24.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aquarian-gnosis.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "dependencies": {
    "sharp": "^0.34.3"
  }
}
